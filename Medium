#include<stdio.h>
int main(){
int num,rem;
int reversed=0;
int original = num;
scanf("%d",&num);
while (num!=0){
rem= num%10;
reversed =reversed *10+rem;
num=num/10;}
if( original==reversed ){
printf("%d palindrome number\n",original);
}
else{
(" %d not palindrome number \n",original);
}
return 0;
}

